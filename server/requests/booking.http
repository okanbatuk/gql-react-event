### GET ALL BOOKINGS
###
POST http://localhost:4000/graphql HTTP/1.1
X-Request-Type: GraphQL
Content-Type: application/json
Authorization: Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VyIjoiZGFybWlhbkBleGFtcGxlLmNvbSIsImlhdCI6MTY5MDczNzIyNywiZXhwIjoxNjkwNzQwODI3fQ.K7vMjVm58JJ7mzh3WYxKCvi8lzL7fytNwuQEJLJi0mo

query {
  bookings {
    _id
    createdAt
    updatedAt
    event {
      title
      description
      price
      date
    }
    user {
      email
      createdAt
      updatedAt
    }
  }
}

### BOOK THE EVENT
###
POST http://localhost:4000/graphql HTTP/1.1
Content-Type: application/json
X-REQUEST-TYPE: GraphQL
Authorization: Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VyIjoiZGFybWlhbkBleGFtcGxlLmNvbSIsImlhdCI6MTY5MDczNzIyNywiZXhwIjoxNjkwNzQwODI3fQ.K7vMjVm58JJ7mzh3WYxKCvi8lzL7fytNwuQEJLJi0mo

mutation ($event: ID!, $user: ID!) {
  bookEvent(event: $event, user: $user) {
    _id
    createdAt
    updatedAt
  }
}

